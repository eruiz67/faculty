<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="program_course_wizard_view_form" model="ir.ui.view">
        <field name="name">faculty.program_course_wizard.view.form</field>
        <field name="model">faculty.program_course_wizard</field>
        <field name="arch" type="xml">
        <form>
            <group>
                    <field name="date_published" />
            </group>
            <footer>
                <button type="object" name="action_program"
                    string="Programar" class="oe_highlight"/>
                <button special="cancel" string="Cancelar"/>
            </footer>
        </form>
        </field>
    </record>

</odoo>
